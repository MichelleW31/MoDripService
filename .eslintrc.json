{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "max-len": "off"
  }
}
